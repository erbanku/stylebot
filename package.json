{
  "name": "stylebot-next",
  "version": "3.1.4",
  "description": "Change the appearance of the web instantly - Modern Chrome-compatible version",
  "homepage": "https://github.com/erbanku/stylebot#readme",
  "bugs": {
    "url": "https://github.com/erbanku/stylebot/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erbanku/stylebot.git"
  },
  "license": "MIT",
  "author": "Ankit Ahuja",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "build:firefox": "cross-env NODE_ENV=production BROWSER=firefox webpack",
    "watch": "cross-env NODE_ENV=development webpack --watch",
    "watch:firefox": "cross-env NODE_ENV=development BROWSER=firefox webpack --watch",
    "start:firefox": "web-ext run --source-dir ./firefox-dist/",
    "lint": "eslint --ext .js,.ts,.vue src",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "postinstall": "patch-package"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "setupFiles": [
      "./setupJest.js"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@stylebot/(.*)$": "<rootDir>/src/$1/index",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "yarn lint:fix"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.24.0",
    "@types/chrome": "^0.0.268",
    "@types/dedent": "^0.7.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.17.0",
    "@types/tinycolor2": "^1.4.6",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^1.3.6",
    "autoprefixer": "^10.4.23",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.7.0",
    "bootstrap": "^4.6.2",
    "bootstrap-vue": "^2.23.1",
    "copy-webpack-plugin": "^12.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.11.0",
    "cssnano": "^6.1.0",
    "date-fns": "^3.6.0",
    "dedent": "^1.5.0",
    "deepmerge": "^4.3.1",
    "ejs": "^3.1.10",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-vue": "^9.24.0",
    "hotkeys-js": "^3.13.0",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^15.2.0",
    "lodash": "^4.17.23",
    "mini-css-extract-plugin": "^2.9.0",
    "monaco-editor": "^0.48.0",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.0",
    "postcss-rem-to-pixel": "^4.1.2",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.2.0",
    "prettier-eslint": "^16.3.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "readability": "mozilla/readability#main",
    "requirejs": "^2.3.7",
    "sass": "^1.75.0",
    "sass-loader": "^14.2.0",
    "terser-webpack-plugin": "^5.3.0",
    "tinycolor2": "^1.6.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.4.0",
    "vue": "^2.7.16",
    "vue-draggable-resizable": "^2.3.0",
    "vue-eslint-parser": "^9.4.0",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.11.0",
    "vue-swatches": "^2.1.1",
    "vue-template-compiler": "^2.7.16",
    "vuex": "^3.6.2",
    "web-ext": "^8.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.0"
  }
}
